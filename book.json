{
  "title": "pamina",
  "description": "pamina构建工具说明文档",
  "author": "pingan",
  "language": "zh-hans",
  "gitbook": ">=3.0.0",
  "root": ".",
  "styles": {
    "website": "styles/website.css"
  },
  "structure": {
    "readme": "introduction.md"
  },
  "plugins": [
    "-sharing",
    "-lunr",
    "-search",
    "-livereload",
    "search-plus",
    "github",
    "splitter",
    "expandable-chapters-small",
    "anchor-navigation-ex",
    "alerts",
    "advanced-emoji",
    "local-video",
    "theme-custom",
    "favicon",
    "code"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com"
    },
    "anchor-navigation-ex": {
      "showLevel": false,
      "float": {
          "showLevelIcon": true,
          "level1Icon": "fa fa-hand-o-right",
          "level2Icon": "fa fa-hand-o-right",
          "level3Icon": "fa fa-hand-o-right"
      }
    },
    "theme-custom": {
      "navbar": {
        "brand": {
            "url": "https://github.com",
            "logo": {
                "alt": "图片信息",
                "path": "assets/cml-logo.png"
            }
        },
        "nav": [
            {
              "text": "快速上手",
              "url": "/quick_start/index.html",
              "part": "quick_start"
            },
            {
              "text": "框架",
              "url": "/frameWork/index.html",
              "part": "framework"
            },
            {
              "text": "组件",
              "url": "/component/index.html",
              "part": "component"
            },
            {
              "text": "API",
              "url": "/api/index.html",
              "part": "api"
            }
        ]
      }
    },
    "favicon": {
      "shortcut": "assets/favicon.ico",
      "bookmark": "assets/favicon.ico"
    }
  }
}